{
  "name": "wincalx-web-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.2.1",
    "antd": "^3.26.18",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "cross-env": "^6.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "flow-bin": "^0.115.0",
    "formik": "^2.1.4",
    "jwt-decode": "^2.2.0",
    "moment": "^2.27.0",
    "node-sass": "^7.0.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.2.0",
    "react-slick": "^0.26.1",
    "react-test-renderer": "^16.13.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "slick-carousel": "^1.8.1",
    "stylelint": "^12.0.0"
  },
  "scripts": {
    "start": "cross-env REACT_APP_CLIENT_ENV=development craco start",
    "start:staging": "cross-env REACT_APP_CLIENT_ENV=staging craco start",
    "start:production": "cross-env REACT_APP_CLIENT_ENV=production craco start",
    "build": "cross-env REACT_APP_CLIENT_ENV=production craco build",
    "test": "cross-env REACT_APP_CLIENT_ENV=test craco test",
    "flow": "flow",
    "lint": "npm run lint:js && npm run lint:style && npm run prettier",
    "lint:js": "eslint --cache --fix --ext .js,.jsx ./src",
    "lint:style": "stylelint --fix \"src/**/*.scss\" --syntax scss",
    "prettier": "prettier -c --write \"**/src/**\"",
    "ci": "npm set progress=false && npm install && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,scss,md,json}": [
      "npm run prettier",
      "git add"
    ],
    "**/*.{js,jsx}": [
      "npm run lint:js",
      "git add"
    ],
    "**/*.scss": [
      "npm run lint:style",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "empty": "^0.10.1",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-babel": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-unicorn": "^13.0.0",
    "husky": "^3.1.0",
    "jsdoc": "^3.6.4",
    "lint-staged": "^9.4.3",
    "moxios": "^0.4.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.2",
    "redux-mock-store": "^1.5.4",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-prettier": "^1.1.2",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpackbar": "^4.0.0"
  }
}
